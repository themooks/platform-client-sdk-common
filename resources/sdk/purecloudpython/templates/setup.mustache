[build-system]
requires = ["setuptools"]
build-backend = "setuptools.build_meta"

[project]
name = "{{packageName}}"
version = "{{packageVersion}}"
authors = [
    {name = "{{packageAuthor}}", email = "{{packageAuthorEmail}}"},
]
description = "{{packageTitle}}"
readme = "README.md"
requires-python = ">=3.9"
keywords = ["Swagger {{packageKeywords}}"]
classifiers = [
    "License :: OSI Approved :: MIT License",
    "Programming Language :: Python"
]
license = {file = "License.txt"}
dependencies = [
    "certifi>=2025.01.31",
    "urllib3>=2.2.0",
    "python-dateutil>=2.9.0.post0",
    "watchdog>=4.0.0"
]

[project.urls]
Homepage = "{{packageUrl}}"

[tool.setuptools]
include-package-data = true
